{
    "extends": ["airbnb-base", "prettier"],
    "plugins": ["prettier", "jest"],
    "rules": {
        "prettier/prettier": [
            "error",
            {
              "endOfLine": "auto"
            }
        ],
        "import/prefer-default-export": "off"
    },
    "env": {
        "jest/globals": true
    }
}